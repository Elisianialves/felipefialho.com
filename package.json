  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^5.14.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.5.0",
    "husky": "^3.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
