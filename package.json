    "gatsby-transformer-sharp": "^2.2.5",
    "prismjs": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "styled-components": "^4.3.2",
    "styled-icons": "^8.3.0",
    "styled-media-query": "^2.1.2"
