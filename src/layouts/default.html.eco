<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
	<!-- Standard Meta -->
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta name="viewport" content="width=device-width" />

	<!-- Site Properities -->
	<title><%= @getPreparedTitle() %></title>
	<meta name="description" content="<%= @getPreparedDescription() %>" />
	<meta name="keywords" content="<%= @getPreparedKeywords() %>" />

	<!-- Meta -->
	<%- @getBlock('meta').toHTML() %>

	<!-- Modernizr -->
	<%- @getBlock('scripts').add(@site.scriptsTop).toHTML() %>

	<!-- Styles + Our Own -->
	<%- @getBlock('styles').add(@site.styles).toHTML() %>

	<!-- Favicon -->
	<link rel="shortcut icon" href="<%= @getRelativeFile() %>img/icons/favicon2.ico" />
</head>
<body>
	<div class="row-fluid">

		<!-- SIDE -->
		<aside class="span3">
			<div class="navbar nav-side">
				<!-- HEADER -->
				<header>
					<div class="photo">
						<img src="<%= @getRelativeFile() %>img/default/luiz-felipe-tartarotti-fialho.png" alt="Cartoon de Luiz Felipe Tartarotti Fialho">
					</div>
					<h1 class="logo">
						<a href="/">Felipe Fialho</a>
					</h1>	
					<h4>Front-End Developer</h4>
				</header>
				<!-- END HEADER -->
				<!-- NAV -->
				<%-@partial('nav.html.eco', @)%>
				<!-- END NAV -->
				<!-- SOCIAL MEDIA -->
				<ul class="social-media inline">
					<li><a href="https://twitter.com/LFeh"><span class="icon-twitter" aria-hidden="true"></span></a></li>
					<li><a href="https://github.com/LFeh"><span class="icon-github-2" aria-hidden="true"></span></a></li>
					<li><a href="http://www.linkedin.com/in/lfehfrontend"><span class="icon-linkedin" aria-hidden="true"></span></a></li>
					<li class="hide"><a href="http://portfolio.felipefialho.com/"><span class="icon-linkedin" aria-hidden="true"></span></a></li>
				</ul>	
				<!-- END OF SOCIAL MEDIA -->
			</div>	
		</aside>
		<!-- END SIDE -->
		<section class="wrap span9">
			<article>
				<%- @content %>
			</article>
		</section>
		<a class="fork" href="https://github.com/LFeh/Website"><img alt="Fork me on GitHub" src="<%= @getRelativeFile() %>img/default/fork.png"></a>
	</div>	
	<!-- Scripts -->
	<%- @getBlock('scripts').add(@site.scripts).toHTML() %>
	<!--[if IE 7]>
		<%- @getBlock("scripts").add(["/scripts/plugin/lte-ie7.js"]).toHTML() %>
	<![endif]-->
	<script>
	  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

	  ga('create', 'UA-40410936-1', 'felipefialho.com');
	  ga('send', 'pageview');

	</script>
</body>
</html>